<!DOCTYPE html>
<html>
<head>
    <% include ../partials/head %>
</head>

<body class="hm">
    <main class="hm-main-view">
        <section class="hm-mr-0-auto hm-max-w-800">
            <div class="text-center pt-5">
                <a href="">
                    <img src="<%= baseUrl %>/img/logo.png" alt="" height="24">
                </a>
            </div>
            <div class="card p-4">
                <h3 class="hm-mr-0">
                    Company Registration
                </h3>
                <form class="form" method="" action="">
                    <div class="">
                        <div class="row">
                            <div class="col-12 col-md-6">
                                <div class="form-group ">
                                    <label class="bmd-label-floating"><sup class="text-danger">*</sup>Name</label>
                                    <input type="text" class="form-control">
                                    <div class="hm-input-error-msg">
                                        Required Field!
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6">
                                <div class="form-group ">
                                    <label class="bmd-label-floating"><sup class="text-danger">*</sup>Business Email</label>
                                    <input type="email" class="form-control">
                                    <div class="hm-input-error-msg">
                                        Required Field!
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-12 col-md-6">
                                <div class="form-group ">
                                    <label class="bmd-label-floating"><sup class="text-danger">*</sup>Company</label>
                                    <input type="text" class="form-control">
                                    <div class="hm-input-error-msg">
                                        Required Field!
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-md-6">
                                <div class="form-group ">
                                    <label class="bmd-label-floating"><sup class="text-danger">*</sup>Phone</label>
                                    <input type="tel" class="form-control">
                                    <div class="hm-input-error-msg">
                                        Required Field!
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="" class="">Industry Type</label>
                            <select id="" class="form-control">
                                <option selected disabled="">Choose...</option>
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                            </select>
                        </div>
                    </div>
                    <div class=" mt-4 ">
                        <div class="row no-gutters">
                            <div class="hm-col">
                                <div class="form-check">
                                    <label class="form-check-label">
                                        <input class="form-check-input" type="checkbox" value=""> Accept 
                                        <span class="form-check-sign">
                                            <span class="check"></span>
                                        </span>
                                    </label>
                                    <a href="">Terms & Conditions</a>
                                </div>
                            </div>
                            <div class="pl-3">
                                <button type="button" class="btn btn-primary " onclick="dummyRegistration()">
                                    Register Now
                                </button>
                            </div>
                        </div>
                    </div>
                </form>

            </div>
            <div class="text-center">
                <span class="text-muted">
                    Already have an account?
                </span>
                <a href="/login">Login here.</a>
            </div>

        </section>
    </main>
    <footer>
        <% include ../partials/footer %>
        <script>

        </script>
    </footer>
</body>
</html>
